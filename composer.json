{
    "name": "satyam147/laravel-api-client",
    "description": "Laravel API Client Builder package",
    "keywords": [
        "macsidigital",
        "satyam147",
        "laravel-api-client"
    ],
    "homepage": "https://github.com/satyam147/laravel-api-client",
    "license": "MIT",
    "type": "library",
    "authors": [{
        "name": "Colin Hall",
        "email": "colin@macsi.co.uk"
    }],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "nesbot/carbon": "^2.0|^3.0",
        "guzzlehttp/guzzle": "~7.0",
        "guzzlehttp/oauth-subscriber": "0.8.1.0",
        "firebase/php-jwt": "^6.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "phpunit/phpunit": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "MacsiDigital\\API\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MacsiDigital\\API\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MacsiDigital\\API\\Providers\\APIServiceProvider"
            ],
            "aliases": {

            }
        }
    }
}
